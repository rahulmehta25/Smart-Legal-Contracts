# Test Environment Configuration
NODE_ENV=test
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000/ws

# Test Database Configuration (if needed)
DATABASE_URL=postgresql://arbitration_user:arbitration_pass@localhost:5432/arbitration_test

# Test Settings
VITE_APP_NAME=Arbitration Detector (Test)
VITE_APP_VERSION=1.0.0-test
VITE_ENABLE_DEBUG=true

# Feature Flags for Testing
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_REAL_TIME_ANALYSIS=true
VITE_ENABLE_ANALYTICS=false

# Performance Settings
VITE_API_TIMEOUT=10000
VITE_WEBSOCKET_RECONNECT_INTERVAL=5000

# Mock Settings
VITE_USE_MOCK_API=false
VITE_USE_MOCK_WEBSOCKET=false